<!DOCTYPE lenguaje[
<!ELEMENT lenguaje (codigo+)>
<!ELEMENT codigo (nombre,ejemplo)>
<!ELEMENT nombre (#PCDATA)>
<!ELEMENT ejemplo (#PCDATA)>
]>
<lenguaje>
    <codigo>
        <nombre>HTML</nombre>
        <ejemplo>
          <![CDATA[
            <!DOCTYPE html>
            <html>
             <body>
        
                <h1>My First Heading</h1>
                <p>My first paragraph</p>
        
             </body>
         </html>
        ]]>  
        </ejemplo>
    </codigo>
    <codigo>
        <nombre>SQL</nombre>
            <ejemplo>
                <![CDATA[
                    select nombre, apellidos
                    from alumnos;
                ]]> 
            </ejemplo>
    </codigo>
</lenguaje>
